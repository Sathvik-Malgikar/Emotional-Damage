{
    "manifest_version" : 2,
    "version" : "1.0",
    "name" : "Emotional damage",
    "browser_action" : {
        "default_popup" : "index.html",
        "default_title" : "Emotional damage"
    },
    "content_scripts" : [
        {
            "matches" : ["https://*/*","http://*/*"],
    
            "js" : ["logic.js"]
        }
    ],
    "permissions":[
        "tabs",
        "windows",
        "notifications",
        "https://*/*","http://*/*"
    ]
}